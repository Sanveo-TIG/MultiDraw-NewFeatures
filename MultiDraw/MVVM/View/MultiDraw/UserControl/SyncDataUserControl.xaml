                                       Width="328"
                                       Label="Parameters Collection"
                                       IsAllOptionEnable="True" DropDownClosed="UcMultiSelect_DropDownClosed"  ></TIG:MultiSelectUserControl>
        </StackPanel>
        <StackPanel Grid.Row="2">
            <TIG:ButtonUserControl Label="Sync"
                                  Width="70"
                                  Icon="Tick"
                                  x:Name="btnsync"
                                   Margin="0,10,10,0"
                                   Visibility="Collapsed"
                                  BtnClick="BtnCheck_Click"></TIG:ButtonUserControl>
        </StackPanel>-->
